{
    "name": "mojave",
    "description": "A library of commonly used JavaScript tools and helpers by Becklyn",
    "license": "BSD-3-Clause",
    "author": "Becklyn Studios <hello@becklyn.com>",
    "homepage": "https://github.com/Becklyn/mojave",
    "repository": {
        "type": "git",
        "url": "git://github.com/Becklyn/mojave.git"
    },
    "bugs": {
        "url": "https://github.com/Becklyn/mojave/issues"
    },
    "version": "5.8.1",
    "scripts": {
        "build": "npm run-script clean && node_modules/.bin/tsc --version && node_modules/.bin/tsc -p tests-tsconfig.json --diagnostics --noEmitOnError --noErrorTruncation --listEmittedFiles --pretty",
        "clean": "node .build/clean.js",
        "dev": "node_modules/.bin/tsc -p tests-tsconfig.json --noEmitOnError --noErrorTruncation --listEmittedFiles --pretty -w",
        "prepublishOnly": "npm run-script build",
        "test": "node_modules/.bin/browserstack-runner"
    },
    "dependencies": {
        "mitt": "^1.1.3",
        "preact": "^10.0.0-rc.1",
        "promise-polyfill": "^8.1.3",
        "ts-toolbelt": "^6.0.0",
        "unfetch": "^4.1.0"
    },
    "devDependencies": {
        "@types/qunit": "^2.9.0",
        "browserstack-runner": "^0.9.0",
        "glob": "^7.1.4",
        "kaba": "^8.1.0",
        "qunit": "^2.9.2",
        "travis-size-report": "^1.1.0",
        "typescript": "^3.5.3"
    },
    "keywords": [
        "becklyn",
        "library",
        "widgets"
    ]
}
